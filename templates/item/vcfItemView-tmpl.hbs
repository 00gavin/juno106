<div class="module__header">VCF</div>
    <div class="module__back">
        <div class="fader vcf">
            <p class="label">FREQ</p>
            <p class="label">RES</p>
            <div class="fader__scale">
                <div class="fader__slot">
                    <div class="fader__knob" data-param="vcf-cutoff" data-value="1">
                        <hr>
                    </div>
                </div>
                <div class="fader__slot">
                    <div class="fader__knob" data-param="vcf-res" data-value="0">
                        <hr>
                    </div>
                </div>
            </div>
        </div>
        <div class="knob-container--pwm">
            <p>^</p>
            <div class="switch" data-param="vcf-invert" data-value="1">
                <div class="switch__knob">
                </div>
            </div>
            <p>v</p>
        </div>
        <div class="fader three">
            <p class="label">ENV</p>
            <p class="label">LFO</p>
            <p class="label">KBD</p>
            <div class="fader__scale">
                <div class="fader__slot">
                    <div class="fader__knob" data-param="vcf-envMod" data-value="0">
                        <hr>
                    </div>
                </div>
                <div class="fader__slot">
                    <div class="fader__knob" data-param="lfo-freqMod" data-value="0">
                        <hr>
                    </div>
                </div>
                <div class="fader__slot">
                    <div class="fader__knob">
                        <hr>
                    </div>
                </div>
            </div>
        </div>
    </div>